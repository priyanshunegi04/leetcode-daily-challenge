  string longestPalindrome(string s) {
        int i  = 0;
        int j = s.length()-1;
   
        string len ;
    if(s.length()<2){
        return s;
    }
        
        while(i<j){
 
        
        if(s[i] == s[j]){
            len = s.substr(i, j); 
         if(i<s.length() &&j<s.length()){
            i++;
            j--;
        }
        }
        else{
            i++;
            j--;
        }
        
    }
    return len;
    
    }
